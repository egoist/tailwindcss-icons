{
  "name": "@egoist/tailwindcss-icons",
  "version": "0.0.0",
  "description": "Icons utility for TailwindCSS",
  "publishConfig": {
    "access": "public"
  },
  "type": "module",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      },
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "prepare": "pnpm run gen-types",
    "gen-types": "node ./gen-types.mjs",
    "build-fast": "pnpm gen-types && tsup src/index.ts --format cjs,esm",
    "build": "pnpm run build-fast --dts-resolve",
    "prepublishOnly": "pnpm run build",
    "test": "vitest run",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "lint": "eslint --max-warnings 0 ."
  },
  "license": "MIT",
  "packageManager": "pnpm@8.14.3",
  "devDependencies": {
    "@ianvs/prettier-plugin-sort-imports": "^4.1.1",
    "@iconify-json/heroicons": "^1.1.19",
    "@iconify/json": "^2.2.175",
    "@iconify/types": "^2.0.0",
    "@types/node": "^18.19.10",
    "@typescript-eslint/eslint-plugin": "^6.19.1",
    "@typescript-eslint/parser": "^6.19.1",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "postcss": "^8.4.33",
    "prettier": "^3.2.4",
    "prettier-plugin-tailwindcss": "^0.5.11",
    "tailwindcss": "^3.4.1",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3",
    "vitest": "^1.2.1"
  },
  "dependencies": {
    "@iconify/utils": "^2.1.19"
  },
  "peerDependencies": {
    "tailwindcss": "*"
  }
}
